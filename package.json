{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "Udacity project(2) - Storefront BackEnd app",
  "main": "index.js",
  "scripts": {
    "test": "SET ENV=test&& tsc && jasmine-ts",
    "migrate": "db-migrate up && SET ENV=test&& db-migrate --env test up",
    "test:rs": "set ENV=test&& db-migrate --env test reset",
    "dev": "nodemon src/index.ts",
    "build": "rm -rf dist && tsc",
    "start": "db-migrate && node dist/index.js",
    "watch": "tsc-watch --esModuleInterop src/index.ts --outDir ./dist --onSuccess \"node ./dist/index.js\"",
    "lint": "eslint src/**/*.ts",
    "lint:f": "eslint src/**/*.ts --fix",
    "pret": "prettier -c src/**/*.ts",
    "pret:f": "prettier --write src/**/*.ts"
  },
  "keywords": [],
  "author": "Burham Soliman",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "typescript": "^4.7.4",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/pg": "^7.14.7",
    "@types/prettier": "^2.6.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "tsc-watch": "^4.2.9"
  }
}
